[vps]
# Example 1: Standard root login
# vps1 ansible_host=192.168.1.10 ansible_user=root

# Example 2: Different user and specific key
# vps2 ansible_host=192.168.1.11 ansible_user=admin ansible_ssh_private_key_file=~/.ssh/other_key

# Example 3: Password auth (install sshpass locally first: brew install sshpass)
# vps3 ansible_host=192.168.1.12 ansible_user=debian ansible_password=secret_password

[vps:vars]
ansible_python_interpreter=/usr/bin/python3
